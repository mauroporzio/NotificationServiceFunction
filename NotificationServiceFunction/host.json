{
  "version": "2.0",
  "extensions": {
    "queues": {
      "batchSize": 1,
      "newBatchThreshold": 1
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}